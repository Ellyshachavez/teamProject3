    "body-parser": "^1.17.2",
    "dotenv": "^6.0.0",
    "express": "^4.15.4",
    "if-env": "^1.0.4",
    "mongoose": "^5.0.17",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "xml2js": "^0.4.19",
    "yarn": "^1.10.1"
  }
